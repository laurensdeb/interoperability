{
    "name": "@laurensdeb/authorization-agent-css",
    "version": "1.0.0",
    "author": "Laurens Debackere <Laurens.Debackere@thundr.be> (https://thundr.be)",
    "license": "MIT",
    "private": true,
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "require": "./dist/index.js"
        }
    },
    "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-css",
    "lsd:components": "dist/components/components.jsonld",
    "lsd:contexts": {
        "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-css/^1.0.0/components/context.jsonld": "dist/components/context.jsonld"
    },
    "lsd:importPaths": {
        "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-css/^1.0.0/components/": "dist/components/",
        "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-css/^1.0.0/config/": "config/",
        "https://linkedsoftwaredependencies.org/bundles/npm/@laurensdeb/authorization-agent-css/^1.0.0/dist/": "dist/"
    },
    "scripts": {
        "build": "npm run build:ts && npm run build:components",
        "build:components": "componentsjs-generator -s src/ -c dist/components -i .componentsignore",
        "prepare": "npm run build",
        "build:ts": "tsc",
        "start": "node dist/server/main.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest --coverage",
        "watch": "nodemon --watch \"dist/**/*.js\" --exec npm start"
    },
    "devDependencies": {
        "@tsconfig/node16": "^1.0.2",
        "@types/jest": "^27.4.1",
        "@types/n3": "^1.10.4",
        "@types/node-fetch": "^2.6.1",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "componentsjs-generator": "2.6.1",
        "eslint": "^8.10.0",
        "eslint-config-google": "^0.14.0",
        "jest": "^27.5.1",
        "jest-mock-extended": "^2.0.5",
        "node-mocks-http": "^1.11.0",
        "nodemon": "^2.0.15",
        "ts-jest": "^27.1.3",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@solid/community-server": "^4.0.0",
        "componentsjs": "4.5.0",
        "cross-fetch": "^3.1.5",
        "n3": "^1.16.0",
        "rdf-js": "^4.0.2"
    },
    "files": [
        ".componentsignore",
        "dist",
        "config",
        "templates"
    ]
}
